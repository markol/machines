template< class T1, class T2 >
void perWrite( PerOstream& ostr, const TClass1< T1, T2 >& tc )
{
	ostr << tc.d1_;
	ostr << tc.d2_;
}

template< class T1, class T2 >
void perRead( PerIstream& istr, TClass1< T1, T2 >& tc )
{
	istr >> tc.d1_;
	istr >> tc.d2_;
}
